{
  "schemaVersion": 1,
  "id": "pvp_mod",
  "version": "${version}",
  "name": "pvp mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Terunyann_"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "Apache 2.0",
  "icon": "assets/pvp_mod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.pvp_mod.PvpMod"
    ],
    "client": [
      "com.pvp_mod.PvpModClient"
    ],
    "fabric-datagen": [
      "com.pvp_mod.datagenerator.PvpModDataGenerator"
    ]
  },
  "mixins": [
    "pvp_mod.mixins.json",
    {
      "config": "pvp_mod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.2",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}